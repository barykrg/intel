{
	"name": "intelregistry",
	"displayName": "Intel Registry",
	"description": "Registry for Images hosted by Intel",
	"version": "0.0.5",
	"publisher": "barykrg",
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "Intel Registry",
					"icon": "$(server-environment)"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "catalog",
					"name": "Intel Catalog"
				}
			]
		},
		"commands": [
			
		],
		"menus": {
			"commandPalette": [],
			"view/title": [
			],
			"view/item/context": [
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.45.0",
		"typescript": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2",
		"ftp": "^0.3.10",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"minimist": "^0.2.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/barykrg/Intel-Registry.git"
	}
}
